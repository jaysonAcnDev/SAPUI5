<mvc:View controllerName="testproject.testproject.controller.Detail"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
    <Shell>
        <Page
            title="{i18n>detailPageTitle}"
            showNavButton="true"
		    navButtonPress=".onNavBack">
            <ObjectHeader
            intro="{invoice>ShipperName}"
			title="{invoice>ProductName}"
            />
            <List
            class="sapUiResponsiveMargin"
            width="auto">
                <items>
                    <ObjectListItem                 
                        title="New page, click me!"
                        type="Navigation"
                        press=".onPress" >
                    </ObjectListItem>
                </items>
            </List>
        </Page>
    </Shell>
</mvc:View>